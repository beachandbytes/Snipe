{
	"name": "Snipe",
	"version": "0.2.1",
	"description": "Quickly find the tab you're looking for without ever leaving your keyboard.",
	"permissions": [
	  "tabs", "http://*/*"
	],
	"omnibox": { "keyword" : "tabs" },
	"background_page": "background.html",
	"content_scripts": [{
		"matches": ["http://*/*", "https://*/*"],
		"js": ["lib.js", "snipe.js", "sniperesults.js", "page.js"],
		"run_at": "document_end"
	}],
	"update_url": "http://reusablebits.com/extensions/Snipe.xml"
    // "icons": {
    //  "48": "Icon-48.png"
    // }
}